<h3>以下の内容で、送信する。</h3>
<%= form_with(model: @post, local: true ,url: confirm_posts_path ) do |form| %>
  <%= form.hidden_field :name %>
  <%= form.hidden_field :content %>
  <p>タイトル:<%= @Post.title %></p>
  <p>本文:<%= @Post.content %></p>
  <%= form.submit  "登録する" %>
<% end %>
<%= link_to '戻る', :back %>